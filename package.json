{
  "name": "@alxcube/xml-mapper",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/xml-mapper.umd.cjs",
  "module": "./dist/xml-mapper.js",
  "types": "./dist/xml-mapper.d.ts",
  "exports": {
    ".": {
      "import": "./dist/xml-mapper.js",
      "require": "./dist/xml-mapper.umd.cjs"
    }
