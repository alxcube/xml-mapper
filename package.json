    "@vitest/browser": "^1.3.1",
    "@xmldom/xmldom": "^0.8.10",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-banner": "^0.7.1",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.3.1",
