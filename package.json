    "prettier": "prettier --write 'src/**/*.ts' && prettier --write 'spec/**/*.ts'"
  },
  "author": "Alexander Alexandrov <alxcube@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.19",
